<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="20.0" vgap="20.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="atmp.javafx.EquationController">
   <columnConstraints>
      <ColumnConstraints maxWidth="110.0" minWidth="49.0" prefWidth="87.0" />
      <ColumnConstraints maxWidth="152.0" minWidth="57.0" prefWidth="120.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextField fx:id="aInput" promptText="Значення &quot;а&quot;" />
      <TextField fx:id="bInput" promptText="Значення &quot;b&quot;" GridPane.rowIndex="1" />
      <TextField fx:id="cInput" promptText="Значення &quot;c&quot;" GridPane.rowIndex="2" />
      <Label fx:id="output_1" GridPane.columnIndex="1" />
      <Label fx:id="output_2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" onAction="#rez" prefHeight="25.0" prefWidth="87.0" text="Результат" GridPane.rowIndex="3" />
      <Button mnemonicParsing="false" onAction="#clearData" prefHeight="25.0" prefWidth="87.0" text="Очистити" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
